package rentalMain;

import java.util.Scanner;

public class SpaceRentalMain {

	public static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) {
		
		String hi = "컨테이너 대여공간입니다.";
		
		System.out.println("============================");
		System.out.printf("    %s\n",hi);
		System.out.println("============================");
		
		System.out.println("1. 회원가입");
		System.out.println("2. 로 그인");
		System.out.println("3. 메뉴보기");
		
		System.out.println("============================");
		
		System.out.println("선택>>>");
		int num = sc.nextInt();
		System.out.printf("============= %d 번 ============\n",num);
		
		
	}

}
